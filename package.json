		"package": "concat -o dist/elements.js dist/demo/runtime.js dist/demo/polyfills.js dist/demo/scripts.js dist/demo/main.js",
		"compress": "ngzip dist/elements.js > dist/elements.js.gz",
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --prod --output-hashing=none",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
